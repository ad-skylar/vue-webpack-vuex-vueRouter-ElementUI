{
  "presets": [
      ["env", {
          "modules": false,
          "targets": {
              "browsers": ["iOS >= 7", "Android >= 4.1"]
          }
      }],
      "stage-2"
  ],
  "plugins": [
      "transform-runtime", ["component", [{
              "libraryName": "mint-ui",
              "style": true
          },
          {
              "libraryName": "element-ui",
              "styleLibraryName": "theme-default"
          }
      ]]
  ],
  "env": {
      "test": {
          "presets": ["env", "stage-2"],
          "plugins": ["istanbul"]
      }
  }
}